{
  "name": "mission-control",
  "version": "0.1.0",
  "description": "Orchestration + Observability for Agentic Workflows - Multi-agent mission system with MCP tracking",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:mcp": "pnpm --filter @mission-control/mcp-server dev",
    "dev:ui": "pnpm --filter @mission-control/web-ui dev",
    "build:all": "pnpm --filter @mission-control/shared build && pnpm --filter @mission-control/mcp-server build",
    "db:migrate": "pnpm --filter @mission-control/shared prisma:migrate",
    "db:generate": "pnpm --filter @mission-control/shared prisma:generate",
    "db:studio": "pnpm --filter @mission-control/shared prisma:studio"
  },
  "keywords": [
    "mcp",
    "workflow",
    "observability",
    "agents",
    "claude"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
